{
  "name": "tov-ui-learn-copy",
  "type": "module",
  "version": "1.0.0",
  "description": "this is a vue3 component library",
  "author": "a1345057558 <ismenglz@163.com>",
  "license": "MIT",
  "keywords": [
    "vue3", "vue", "component"
  ],
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./es/*": "./es/*",
    "./dist/*": "./dist/*",
    "./lib/*": "./lib/*",
    "./package.json": "./package.json"
  },
  "main": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "scripts": {
    "build": "vite build",
    "build:umd": "vite build --config vite.umd.config.ts",
    "build:style": "vite build --config vite.style.config.ts"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.5",
    "@tov-ui-learn/utils": "workspace:*",
    "@v-c/utils": "^0.0.26"
  }
}
